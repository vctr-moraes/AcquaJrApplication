@page
@model AcquaJrApplication.Areas.Dashboard.Pages.Clientes.CreateModel
@using AcquaJrApplication.Models

@{
    ViewData["Title"] = "Novo cliente";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<partial name="_DashboardNavPartialView" />

<h3>@ViewData["Title"]</h3>

<hr />
<div class="row">
    <div class="col-md-4">
        @{ await Html.RenderPartialAsync("_ClienteFormPartial", Model.ClienteVM); }
    </div>
</div>

<div>
    <a asp-page="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

<script src="~/js/Cliente.js"></script>
