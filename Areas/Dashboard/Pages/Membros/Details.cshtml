@page
@model AcquaJrApplication.Areas.Dashboard.Pages.Membros.DetailsModel

@{
    ViewData["Title"] = "Detalhes";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<partial name="_DashboardNavPartialView" />

<h3>@ViewData["Title"]</h3>

<div>
    <hr />
    @{ await Html.RenderPartialAsync("_MembroDetalhesPartial", Model.MembroVM); }
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.MembroVM.Id">Editar</a> |
    <a asp-page="./Index">Voltar</a>
</div>
