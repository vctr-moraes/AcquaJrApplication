        <h3>@Html.DisplayFor(model => model.Cargo)</h3>
        <hr />
        <div class="row">
            <div class="col-6">
                <label>Telefone</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Telefone)</p>
            </div>
            <div class="col-6">
                <label>E-mail</label>
                <p class="typography-subheading"> @Model.Email</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>CPF</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Cpf)</p>
            </div>
            <div class="col-6">
                <label>Data de nascimento</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.DataNascimento)</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>Logradouro</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Logradouro)</p>
            </div>
            <div class="col-6">
                <label>Bairro</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Bairro)</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>Cidade</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Cidade)</p>
            </div>
            <div class="col-6">
                <label>CEP</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Cep)</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>Estado</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Estado)</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>Curso</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.Curso)</p>
            </div>
            <div class="col-6">
                <label>Matrícula</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.MatriculaAcademica)</p>
            </div>
        </div>
        <div class="row">
            <div class="col-6">
                <label>Data de entrada</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.DataEntrada)</p>
            </div>
            <div class="col-6">
                <label>Data de saida</label>
                <p class="typography-subheading">@Html.DisplayFor(model => model.DataSaida)</p>
            </div>
        </div>
    </div>
</div>

