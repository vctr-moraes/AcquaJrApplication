@model AcquaJrApplication.ViewsModels.MembroViewModel

<br /><br /><br /><br />
<div class="row">
    <div class="col-4 text-center">
        <img src="~/imagens/ACJR - LOGOO FUNDO BRANCO.jpg" height="200" />
        <br /><br /><br /><br />
        <h5>Acqua Jr. Soluções em Aquicultura</h5>
        <h6>
            Rodovia BR 482 Cachoeiro x Alegre Km 47 <br />
            Caixa Postal 47 - CEP 29500-000 <br />
