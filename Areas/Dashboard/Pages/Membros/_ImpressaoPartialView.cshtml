    _____________________________________________________________________
    <p class="typography-title">@Html.DisplayFor(model => model.Nome)</p>
    <p class="typography-subheading">@Html.DisplayFor(model => model.Cargo)</p>
</div>

<script>
    $(document).ready(function () {
        $('.rodape').remove()
        $('footer').addClass('text-center')
        $('footer').append('Acqua Jr. Soluções em Aquicultura - Rodovia BR 482 Cachoeiro x Alegre Km 47 - Caixa Postal 47 - CEP 29500-000 - Rive - Alegre - Espírito Santo')
        window.print()
    })
</script>
