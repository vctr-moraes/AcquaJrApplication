@page
@model AcquaJrApplication.Areas.Dashboard.Pages.Projetos.EditModel

@{
    ViewData["Title"] = "Editar projeto";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<partial name="_DashboardNavPartialView" />

<h3>@ViewData["Title"]</h3>

<hr />
<partial name="~/Pages/Components/Summary/_ValidationSummaryAllPartial.cshtml" />

<div class="row">
    <div class="col-md-12">
        @{ await Html.RenderPartialAsync("_ProjetoFormPartial", Model.ProjetoVM); }
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script src="~/js/Projeto.js"></script>
}
